{
  "name": "opencart/stripe-payment-gateway",
  "description": "Stripe Payment Gateway for OpenCart 4.x with Payment Intents API",
  "version": "4.0.0",
  "type": "opencart-extension",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "stripe/stripe-php": "^13.0"
  },
  "autoload": {
    "psr-4": {
      "Stripe\\": "upload/system/library/stripe-php/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
